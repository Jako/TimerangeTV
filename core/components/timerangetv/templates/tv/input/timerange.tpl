<table style="margin-left:5px;">  <tr>	<td>{$lang_from}:</td>	<td>		<input id="tv{$tv->id}" name="tv{$tv->id}[]"			type="text" class="textfield"			value="{$tv->get('value')}"			{$style}			tvtype="{$tv->type}"		/>	</td>  </tr>  <tr>	<td>{$lang_to}:</td>	<td>		<input id="tv{$tv->id}_to" name="tv{$tv->id}[]"			type="text" class="textfield"			value="{$tv->get('processedValue')}"			{$style}			tvtype="{$tv->type}"		/>	</td>  </tr></table><script type="text/javascript">{literal}var params = {{/literal}{foreach from=$params key=k item=v name='p'} '{$k}': '{$v|escape:"javascript"}'{if NOT $smarty.foreach.p.last},{/if}{/foreach}{literal}};{/literal}</script><script type="text/javascript">MODx.load(	{ldelim}		xtype: 'timefield',		applyTo: 'tv{$tv->id}',		format: params['timeFormat'] || 'g:i A',		increment: (params['timeIncrement']) ? parseInt(params['timeIncrement']) : 60,		enableKeyEvents: true,		listeners: {ldelim}			'change': {ldelim}				fn:MODx.fireResourceFormChange,				scope:this			{rdelim}		{rdelim}	{rdelim},{ldelim}		xtype: 'timefield',		applyTo: 'tv{$tv->id}_to',		format: params['timeFormat'] || 'g:i A',		increment: (params['timeIncrement']) ? parseInt(params['timeIncrement']) : 60,		enableKeyEvents: true,		listeners: {ldelim}			'change': {ldelim}				fn:MODx.fireResourceFormChange,				scope:this			{rdelim}		{rdelim}	{rdelim});</script>